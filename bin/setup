#!/bin/bash

echo "Deleting previous env if any"
rm -rf .env
echo "Building"
make deps
echo
echo "============================================="
echo "Running Tests, tests might take ~15 seconds"
echo "============================================="
echo
make test
echo "Building"
make build
echo "Creating soft ling for bin/parking_lot"
ln -sf ../.env/bin/runner bin/parking_lot
echo "Run the script to read from file"
./bin/parking_lot file_inputs.txt
